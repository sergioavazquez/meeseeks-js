{
  "name": "meeseeks-js",
  "version": "1.0.1",
  "description": "Useful helper methods for javaScript projects.",
  "main": "lib/meeseeks.js",
  "scripts": {
    "build": "webpack --mode production --config ./config/webpack.config.js",
    "test:unit": "jest unit --no-cache --coverage",
    "test:only": "jest unit --no-cache -t",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test:int": "jest int --no-cache",
    "test:dev": "node --inspect-brk=0.0.0.0 node_modules/.bin/jest --detectOpenHandles",
    "lint": "eslint ./src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergioavazquez/meeseeks-js.git"
  },
  "files": [
    "/lib"
  ],
  "author": {
    "name": "Sergio Vazquez"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sergioavazquez/meeseeks-js/issues"
  },
  "homepage": "https://github.com/sergioavazquez/meeseeks-js#readme",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^3.2.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "prettier-loader": "^3.3.0",
    "regenerator-runtime": "^0.13.9",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-node-externals": "^3.0.0"
  },
  "keywords": [
    "utils",
    "helper functions",
    "to",
    "chain",
    "mock promise",
    "store",
    "watchdog",
    "debounce",
    "pick unique",
    "retry",
    "mock"
  ]
}
